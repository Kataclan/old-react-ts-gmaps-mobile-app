    "version": "0.2.0",
    "configurations": [{
        "name": "Maps - [DEBUG]",
        "type": "chrome",
        "request": "launch",
        "file": "${workspaceRoot}/dist/debug/index.html",
        "sourceMaps": true,
        "userDataDir": "${workspaceRoot}/obj/chrome",
        "sourceMapPathOverrides": {
            "../../src/app/*": "${workspaceRoot}/src/app/*"
        }
    }]
}